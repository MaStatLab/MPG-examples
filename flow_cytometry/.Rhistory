study=data$C1+1,
prior=make.prior(data$Y1, data$n.classes),
mcmc=mcmc,
state=state,
status=TRUE)
alt = mean(fit1$coef["eps"])
fit0 <- HDPMdensity(y=data$Y0,
study=data$C0+1,
prior=make.prior(data$Y1, data$n.classes),
mcmc=mcmc,
state=state,
status=TRUE)
null = mean(fit0$coef["eps"])
write(c(alt,null,i), file = output.path,
ncolumns = 3,
append = TRUE, sep = " ")
}
}
roc.plot = function(out, title_name = "", leg=TRUE)
{
require(ROCR)
